{
	"exclude": ["node_modules"],
	"include": ["**/*.ts", "**/*.tsx"],
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ES2019"],
		"isolatedModules": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"target": "ESNext",
		"skipLibCheck": true,
		"strict": true,
		"baseUrl": ".",
		"noEmit": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true
	}
}
